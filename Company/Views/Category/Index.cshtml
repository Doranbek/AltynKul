@model IEnumerable<Company.Data.Category>

@{
    ViewData["Title"] = "Index";
}

<h1>Список</h1>

<p>
    <a asp-action="Create">Добавить новый</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Категория номера
            </th>
            <th>
                Описание
            </th>
            <th>
                Цена за поток I
            </th>
            <th>
                Цена за поток II
            </th>
            <th>
                Цена за поток III
            </th>
            <th>
                Цена за поток IV
            </th>
            <th>
                Цена за поток V
            </th>
            <th>
                Цена за поток VI
            </th>
            <th>
                Цена за поток VII
            </th>
            <th>
                Цена за поток VIII
            </th>
            <th>
                Цена за поток IX
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Specification)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Camp1)
            </td><td>
                @Html.DisplayFor(modelItem => item.Camp2)
            </td><td>
                @Html.DisplayFor(modelItem => item.Camp3)
            </td><td>
                @Html.DisplayFor(modelItem => item.Camp4)
            </td><td>
                @Html.DisplayFor(modelItem => item.Camp5)
            </td><td>
                @Html.DisplayFor(modelItem => item.Camp6)
            </td><td>
                @Html.DisplayFor(modelItem => item.Camp7)
            </td><td>
                @Html.DisplayFor(modelItem => item.Camp8)
            </td><td>
                @Html.DisplayFor(modelItem => item.Camp9)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Изменить</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
